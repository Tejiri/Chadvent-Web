    <script>
      $("#logoutform").submit(function (e) {
        e.preventDefault();
        location.replace("logout");
      });

      $("#updatepassword").click(function () {
        Swal.fire({
          title: "Retype your new password in the field below to update",
          input: "text",
          inputAttributes: {
            autocapitalize: "off",
          },
          showCancelButton: true,
          confirmButtonText: "Look up",
          showLoaderOnConfirm: true,
        }).then((result) => {
          if (result.isConfirmed) {
            if ($("#newpassword").val() == result.value) {
              $.ajax({
                url: "http://localhost:3000/profile",
                method: "POST",
                data: {
                  newpassword: $("#newpassword").val(),
                },
                success: function (data) {
                  Swal.fire({
                    position: "center",
                    icon: "success",
                    title:
                      "Your password has been updated to " +
                      result.value +
                      ". You are required to sign in again",
                    showConfirmButton: false,
                    timer: 3000,
                  });
                  setTimeout(function () {
                    location.replace("logout");
                  }, 3000);
                },
              });
            } else {
              Swal.fire({
                icon: "error",
                position: "center",
                title: `Passwords do not match`,
              });
            }
          }
        });
      });
    </script>
