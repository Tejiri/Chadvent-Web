              <table class="display blueTable">
                <thead>
                  <th style="text-align: center">S/N</th>
                  <th>Date</th>
                  <th>Narration</th>
                  <th>Account</th>
                  <th>Debit</th>
                  <th>Credit</th>
                  <th>Balance</th>
                </thead>

                <tbody>
                  <%let sn = 1%> <%for (let i = transactionsList.length
                  -1;i>=0;i--) {%> <%if(sn>10){%> <%}else{%>
                  <%if(transactionsList[i].transactiontype == "credit"){%>
                  <tr>
                    <td><%=sn%></td>
                    <td><%=transactionsList[i].date%></td>
                    <td><%=transactionsList[i].narration%></td>
                    <td><%=transactionsList[i].account%></td>
                    <td>0</td>
                    <td>₦<%=transactionsList[i].amount%></td>
                    <td>₦<%=contributionBalance[i]%></td>
                  </tr>
                  <%}else{%>

                  <tr>
                    <td><%=sn%></td>
                    <td><%=transactionsList[i].date%></td>
                    <td><%=transactionsList[i].narration%></td>
                    <td><%=transactionsList[i].account%></td>
                    <td>₦<%=transactionsList[i].amount%></td>
                    <td>0</td>
                    <td>₦<%=contributionBalance[i]%></td>
                  </tr>

                  <%}%> <%}%> <%sn++%> <% }%>
                </tbody>
              </table>

